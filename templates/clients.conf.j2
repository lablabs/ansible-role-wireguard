[Interface]
PrivateKey = [User Private Key File]
Address = {{ item.WireGuardPeer | json_query('[?"AllowedIPs"].AllowedIPs') | first }}

[Peer]
PublicKey = {{ item.WireGuardPeer | json_query('[?"PublicKey"].PublicKey') | first }}
PresharedKey = {{ preshared_key }}
AllowedIPs = {{ allowed_ips }}
Endpoint = {{ wireguard_hostname }}:{{ wireguard_port }}
PersistentKeepalive = {{ wireguard_keepalive }}